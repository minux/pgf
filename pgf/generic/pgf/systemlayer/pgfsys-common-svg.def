\def\pgfsys@declarepattern#1#2#3#4#5#6#7#8#9{%
  % Start building the pattern dictionary:
  \pgf@xa=#2\relax%
  \pgf@ya=#3\relax% 
  \pgf@xb=#4\relax%
  \pgf@yb=#5\relax%
  \pgf@xc=#6\relax%
  \pgf@yc=#7\relax%
  \ifnum#9=1\relax%
    % Colored. That's easy:
    \pgfsysprotocol@literal{
      <pattern
        id="pgfpat#1"
        patternUnits="userSpaceOnUse"
        width="\the\pgf@xc"
        height="\the\pgf@yc">
        #8
      </pattern>}%
  \else%
    % Uncolored. Yikes!
    \pgfsysprotocol@literal{
      <pattern
        id="pgfpat#1"
        patternUnits="userSpaceOnUse"
        width="\the\pgf@xc"
        height="\the\pgf@yc"/>
      <symbol id="pgfsym#1">
        #8
      </symbol>}%
  \fi%
}

\def\pgfsys@setpatternuncolored#1#2#3#4{%
  \global\advance\pgf@sys@svg@objectcount by1\relax%
